{
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": true
		},
		"target": "es2020",
		"loose": false,
		"externalHelpers": false,
		"transform": {
			"optimizer": {
				"globals": {
					"vars": {
						"__DEV__": "true"
					}
				}
			}
		}
	},
	"module": {
		"type": "commonjs"
	},
	"minify": false,
	"sourceMaps": true
}
